{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Keycloak": {
    "realm": "my_realm",
    "auth-server-url": "http://host.docker.internal:8484/auth/",
    "ssl-required": "none",
    "resource": "mytask-client",
    "verify-token-audience": false,
    "confidential-port": 0
  },
  "ConnectionStrings": {
    "Mongo": "mongodb://user:pass@host.docker.internal:27017/"
  },
  "AllowedHosts": "*"
}
